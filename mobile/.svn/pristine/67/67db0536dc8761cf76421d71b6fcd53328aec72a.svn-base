/*活动区块*/
.activityBody{padding:5px 10px 0 10px;background-color:#f2f2f2;}

/*标题*/
.block-title{text-align:center;font-size:16px;line-height:40px;background:url(https://api.25boy.cn/Public/images/title-line.jpg) no-repeat bottom center;background-size:contain;padding-bottom:12px;margin-bottom:10px;}

/*新分类*/
.categoryList{clear:both;overflow:hidden;padding:0 5px;}
.categoryItem{width:25%;float:left;}
.categoryItem>a{display:block;padding:5px;}

/*联名合作区*/
.jointName{clear:both;overflow:hidden;}
.joint-group0{float:left;width:61%;}
.joint-group0 .joint-item1{margin-top:0.6em;}
.joint-group1{float:right;width:38.9%}
.joint-group2{margin-top:2px;float:left;}
.joint-group2 .joint-item0{float:left;width:61%;}
.joint-group2 .joint-item1{float:right;width:38.9%;}
.joint-item a{display:block}
.joint-img{width:100%;display:block;}
