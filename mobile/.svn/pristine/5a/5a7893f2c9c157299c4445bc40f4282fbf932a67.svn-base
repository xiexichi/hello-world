/* 点击弹出半环形菜单 效果CSS开始 */
.js-wrap{ width: 52px; height: 57px; position: fixed; right: 20px; bottom: 20px;}
.icon_menu{width:57px;height:57px;position:absolute;bottom:0;right:0;z-index:10;cursor:pointer;}
.icon_menu span{display:none;}
.js-btn{width:52px;height:57px;background:url(/statics/img/bg1.png) no-repeat -78px -6px;z-index:1;position:absolute;bottom:0;right:0;margin:0 4px -4px 0;cursor:pointer;}
.js-btn span{display:block;width:22px;height:24px;font-size:0;position:absolute;top:50%;left:50%;margin-top:-12px;margin-left:-11px;overflow:hidden;background:url(/statics/img/bg1.png) no-repeat;}
.js-btn1 span{background-position:-176px -20px;}
.js-btn2 span{background-position:-198px -18px;}
.js-btn3 span{background-position:-221px -18px;}
.js-btn4 span{background-position:-243px -19px;}
.js-btn5 span{background-position:-267px -20px;}
.js-btn{-webkit-transition:bottom .2s,right .2s;-moz-transition:bottom .2s,right .2s;-o-transition:bottom .2s,right .2s;-ms-transition:bottom .2s,right .2s;transition:bottom .2s,right .2s;}
.js-btn1 .close, .js-btn2 .close, .js-btn3 .close, .js-btn4 .close{bottom:20px;right:20px;}
.js-btn1.open{bottom:115px;right:0;}
.js-btn2.open{bottom:90px;right:54px;}
.js-btn3.open{bottom:55px;right:97px;}
.js-btn4.open{bottom:0px;right:115px;}
.js-wrap img {
	max-width:110%;
}
/* 效果CSS结束 */

