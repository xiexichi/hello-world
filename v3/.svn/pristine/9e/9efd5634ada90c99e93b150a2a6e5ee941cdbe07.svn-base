/*靠两边居中对齐*/
.fled-space-between{
  display: flex;
  justify-content: space-between;
  align-items: center;
}

/*靠两边上对齐*/
.fled-between-flex-start{
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
}

/*靠左上对齐*/
.fled-start-flex-start{
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
}

/*靠左下对齐*/
.fled-between-flex-end{
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
}

/*靠左，X轴对齐*/
.fled-flex-start{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
  align-content: space-around;
}

.fled-flex-end{
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.fled-flex-center{
