  },
  mounted: function () {
  },
	computed: {
    // 数量统计
    qtyTotal: function() {
      const items = this.items
      let spuObject = []
      let skuNum = 0
      for (var i in items) {
        skuNum += parseInt(items[i].quantity)
        if (spuObject.indexOf(items[i].item_code) === -1)
          spuObject.push(items[i].item_code)
      }
      return {
        spu: spuObject.length,
        sku: skuNum
      }
    }
	},
