@import 'style/weui.wxss';
@import 'style/csshake.min.wxss';


/*全局*/
page{font-family:"微软雅黑","黑体","Myriad Set Pro",simsun,"宋体","Lucida Grande",Helvetica,sans-serif;}
contact-button{z-index:999;}
.display-none{display:none !important;}
.display-block{display:block !important;}
.display-flex{display:flex !important;}
.img-box image{width:100%;display:block;}
.pagegrey{background-color:#f2f2f2 !important;}
.blank1{clear:both;height:1px;overflow:hidden;display:block;font-size:1px}
.blank2{clear:both;height:2px;overflow:hidden;display:block;font-size:1px}
.blank5{clear:both;height:5px;overflow:hidden;display:block;font-size:1px}
.blank10{clear:both;height:10px;overflow:hidden;display:block;font-size:1px}
.blank15{clear:both;height:15px;overflow:hidden;display:block;font-size:1px}
.blank20{clear:both;height:20px;overflow:hidden;display:block;font-size:1px}
.blank40{clear:both;height:40px;overflow:hidden;display:block;font-size:1px}
.blank30{clear:both;height:30px;overflow:hidden;display:block;font-size:1px}
.color-red{color:red !important;}
.color-gray{color:#999 !important;}
.color-link{color:#169BD5 !important;}
.bold{font-weight:700;}
.hover-class{opacity:0.7;}
.btn-text{
  padding:0;
  line-height:inherit;
  background-color: transparent;
  border:none;
  font:inherit;
  text-align: inherit;
}
.btn-text::after {
  border-width: 0px;
}


/*头部*/
.page-header{border-bottom: 1rpx solid #CFCFCF; height:40px;line-height:40px;background-color:#fff;display:flex;justify-content:space-between;padding:0 15px;position:relative;}
.page-topbar-left,
.page-topbar-right{text-align:center;overflow:hidden;display:flex;z-index:2;position:relative;}
.page-topbar-right{text-align:right;}
.page-header-icon{width:26px;height:26px;display:inline-block;margin-top:6px;}
.page-header-text{padding:0 30px;text-align:center; font-size:30rpx;text-overflow:ellipsis;white-space:nowrap;overflow:hidden;}
.header-icon-txt{font-size:32rpx;align-items:center;line-height:40px;margin-left:3px;color:#b01f23;}
.page-header-max{height:40px;line-height:40px;}
.page-header-max .page-header-icon{width:26px;height:26px;margin-top:6px;}
.page-header-max .header-icon-txt{font-size:32rpx;line-height:40px;}
.page-header-filter{text-align:center;position:absolute;left:0;top:0;width:100%;z-index:1;}
.page-header-filter-text{display:inline-block;background:url(https://api.25boy.cn/Public/images/icon-filter.png) no-repeat right center;background-size:12px;padding-right:14px;font-size:0.9em;}

/*布局*/
.flex-wrp{
    display:flex;
    width:100%;
    flex-direction:row;
}
.flex-item{
    display:flex;
    justify-content: center;
    align-items: center;
}
.flex-col-2 .flex-item{
  width:50%;
}
.flex-col-3 .flex-item{
  width:33.3%;
}
.flex-col-4 .flex-item{
  width:25%;
}
.flex-col-5 .flex-item{
  width:20%;
}

/*对齐*/
.jbw{
  -moz-display:flex;-webkit-display:-webkit-box;-webkit-display:flex;-o-display:flex;-ms-display:flex;display:-webkit-box;display:flex;
  -moz-justify-content:space-between;
  -o-justify-content:space-between;
  -ms-justify-content:space-between;
  -webkit-box-pack:justify;
  justify-content:space-between;
}
.jbc{
  -moz-display:flex;-webkit-display:-webkit-box;-webkit-display:flex;-o-display:flex;-ms-display:flex;display:-webkit-box;display:flex;
  -moz-justify-content:center;
  -o-justify-content:center;
  -ms-justify-content:center;
  -webkit-box-pack:justify;
  justify-content:center;
}

/*加载更多*/
.loadmore{
    width: 65%; margin:1.5em auto; line-height: 1.6em; font-size:14px; text-align: center;
    position:fixed;bottom:0;left:0;width:100%;background:#fff;margin:0;padding:1em 0;opacity:0.9;border-top:1px solid #ddd;z-index:99;
}
.loadmore text{vertical-align:middle;display:inline-block;color:#888888;}
.loadmore-icon {
  width:20px;
  height:20px;
  display: inline-block;
  vertical-align: middle;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  -webkit-background-size: 100%;
  background-size: 100%;
}
.loadmore_line{
    width:65%;
    border-top:1px solid #dddddd;
    margin:1.5em auto;
    margin-top:2.4em;
    line-height: 1.6em;
    font-size:14px;
    text-align: center;
}
.loadmore_line text{
    position: relative;
    top:-.9em;
    padding:0 .55em;
    background-color: #f2f2f2;
    color:#999999;
}

/*弹出层*/
.mask{ position: fixed;
  z-index: 1000;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6);
  opacity:0;
  display:none;
}
.mask.show{opacity:1;display:block;}
.popLayer{position:fixed;bottom:0;left:0;width:100%;z-index: 5000;
  -webkit-transform: translate(0, 100%);
  transform: translate(0, 100%);
  -webkit-backface-visibility: hidden;
  backface-visibility: hidden;
  -webkit-transition: -webkit-transform .3s;
  transition: -webkit-transform .3s;
  transition: transform .3s;
  transition: transform .3s, -webkit-transform .3s;
}
.popLayer.show{-webkit-transform: translate(0, 0); transform: translate(0, 0);}
.popLayer .close{position:absolute;right:20rpx;top:20rpx;}
.popLayer-right{-webkit-transform: translate(100%, 0); transform: translate(100%, 0);}
.popLayer-top{-webkit-transform: translate(0, -100%); transform: translate(0, -100%);bottom:inherit;top:0;}
.popLayer-title{text-align:center;padding:30rpx 0;background-color:#f2f2f2;font-size:32rpx;}

/*更多小箭头*/
.arrow{position:relative;}
.arrow:after {
  content: " ";
  display: inline-block;
  height: 6px;
  width: 6px;
  border-width: 2px 2px 0 0;
  border-color: #C8C8CD;
  border-style: solid;
  -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
          transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  top: -2px;
  position: absolute;
  top: 50%;
  right:30rpx;
  margin-top: -3px;
}
.arrow2{position:relative;}
.arrow2:after {
  content: " ";
  display: inline-block;
  height: 6px;
  width: 6px;
  border-width: 0 2px 2px 0;
  border-color: #C8C8CD;
  border-style: solid;
  -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
          transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  top: -2px;
  position: absolute;
  top: 50%;
  right:30rpx;
  margin-top: -4px;
}

/*购物车与订单底部*/
.cartBottom{position:fixed;bottom:0;left:0;width:100%;background:#fff;padding:20rpx 0;height:80rpx;z-index:9;}
.cartBottom .selectAll{float:left;padding-left:20rpx;font-size:28rpx;padding-top:10rpx;}
/*.cartBottom .selectAll icon{position:relative;top:5px;}*/
.cartBottom .barResult{float:right;display:flex;padding-right:20rpx;}
.cartBottom .total{color:#999;font-size:28rpx;margin-right:20rpx;line-height:80rpx;}
.cartBottom .subtotal{color:#b01f23;font-size:42rpx;margin-left:8rpx;font-weight:700;}
.cartBottom button{line-height:80rpx;padding-left:50rpx;padding-right:50rpx;}

.fixed-bottom-button{position:fixed;bottom:0;z-index:1;left:0;width:100%;background-color:#e45659;color:#fff;text-align:center;font-size:34rpx;height:45px;line-height:45px;}

.floatHomeBtn-Area{position:fixed;width:100%;height:100%;left:0;top:0;background:transparent;z-index:0;}
.floatHomeBtn{bottom:160rpx;left:660rpx;width:60rpx;height:60rpx;overflow:hidden;opacity:0.45;z-index:100;position:fixed;}
.floatHomeBtn-img{width:60rpx;height:60rpx;}


/* 表格 */
.table{display:table;width:100%;background-color:#fff;}
.table-row{display:table-row;}
.table-th{font-weight:700;border-bottom:1px #ddd solid;text-align:center;display:table-cell;padding:10px;}
.table-td{font-size:28rpx;text-align:center;display:table-cell;padding:10px;}


/* 重写weui */
.weui-navbar{position:relative;color:#999;}
.weui-navbar__title{display:block;max-width:none;}
.weui-navbar__item{font-size:30rpx;}
.weui-form-preview__span{color:#b01f23;font-size:0.5em;display:inline;}
.weui-grids-2 .weui-grid{position: relative;width: 50%;}
.weui-grids-4 .weui-grid{position: relative;width: 25%;}


/* 提现申请弹出提示 */
.promoteTipsLayer{width:100%;height:100%;display:flex;align-items:center;justify-content:center;}
.promoteTips{background-color:#fff;padding:30rpx;border-radius:6px;width:60%;text-align:center;margin:auto;}
.promoteTips-image{width:100%;margin-bottom:20rpx;}
.promoteTips-cancel{text-align:center;color:#999;font-size:26rpx;margin-top:18rpx;}
.promoteTipsLayer .popup-box{text-align:center;}
.promoteTipsLayer .popup-image{max-width:640rpx;display:block;}
